Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"SP", "[", 
    RowBox[{"p1", ",", "p1"}], "]"}], "=", 
   SuperscriptBox["mf", "2"]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"SP", "[", 
    RowBox[{"p2", ",", "p2"}], "]"}], "=", 
   SuperscriptBox["mf", "2"]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"SP", "[", 
    RowBox[{"p1", ",", "p2"}], "]"}], "=", 
   RowBox[{
    RowBox[{
     FractionBox["1", "2"], 
     SuperscriptBox["mh", "2"]}], "-", 
    SuperscriptBox["mf", "2"]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"M", "=", 
   RowBox[{"\[ImaginaryI]", " ", 
    FractionBox["mf", "v"], 
    RowBox[{
     RowBox[{"Spinor", "[", 
      RowBox[{
       RowBox[{"Momentum", "[", "p2", "]"}], ",", "mf"}], "]"}], ".", 
     RowBox[{"Spinor", "[", 
      RowBox[{
       RowBox[{"-", 
        RowBox[{"Momentum", "[", "p1", "]"}]}], ",", "mf"}], "]"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Msq", "=", 
  RowBox[{
   RowBox[{"DiracSimplify", "[", 
    RowBox[{"FermionSpinSum", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"ComplexConjugate", "[", "M", "]"}], "*", "M"}], ",", 
      RowBox[{"ExtraFactor", "\[Rule]", 
       FractionBox["1", "4"]}]}], "]"}], "]"}], "//", 
